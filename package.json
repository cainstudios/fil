{
  "name": "fil",
  "version": "4.0.2",
  "description": "A superfast static site generator",
  "scripts": {
    "compile": "shx rm -r compiled && babel app -d compiled",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "nyc --all spec/run.js"
  },
  "bin": {
    "fil": "./compiled/bin/index.js"
  },
  "author": "Umut Benzer <umut@benzer.me>",
  "license": "MIT",
  "dependencies": {
    "@material/button": "^0.3.6",
    "browser-sync": "^2.18.12",
    "debug": "^2.6.8",
    "deep-eql": "^2.0.2",
    "deep-iterator": "^1.1.0",
    "dot-prop-immutable": "^1.3.1",
    "fs-extra": "^3.0.1",
    "gauge": "^2.7.4",
    "minimist": "^1.2.0",
    "npid": "^0.4.0",
    "rxjs": "^5.4.0",
    "sanitize-filename": "^1.6.1",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "codecov": "^2.2.0",
    "eslint": "^3.19.0",
    "jasmine": "^2.6.0",
    "jasmine-promise-wrapper": "0.0.2",
    "nyc": "^10.3.2",
    "shx": "^0.2.2",
    "source-map-support": "^0.4.15"
  },
  "keywords": [
    "static-site",
    "jekyll",
    "hexo",
    "blog",
    "wordpress",
    "collections",
    "taxonomy"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ubenzer/fil.git"
  },
  "bugs": {
    "url": "https://github.com/ubenzer/fil/issues"
  },
  "preferGlobal": true,
  "nyc": {
    "include": [
      "app/**"
    ],
    "reporter": [
      "lcov",
      "text",
      "json"
    ]
  }
}
