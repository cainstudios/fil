{
  "name": "fil",
  "version": "8.0.0",
  "description": "A superfast static site generator",
  "scripts": {
    "compile": "shx rm -r compiled && babel app -d compiled",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "nyc --all spec/run.js"
  },
  "bin": {
    "fil": "./compiled/bin/index.js"
  },
  "author": "Umut Benzer <umut@benzer.me>",
  "license": "MIT",
  "dependencies": {
    "browser-sync": "^2.18.13",
    "debug": "^3.0.1",
    "fs-extra": "^4.0.1",
    "gauge": "^2.7.4",
    "lodash.isplainobject": "^4.0.6",
    "mime-types": "^2.1.16",
    "minimist": "^1.2.0",
    "npid": "^0.4.0",
    "rxjs": "^5.4.3",
    "sanitize-filename": "^1.6.1",
    "supports-color": "^4.2.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "codecov": "^2.3.0",
    "eslint": "^4.5.0",
    "jasmine": "^2.8.0",
    "jasmine-promise-wrapper": "0.0.3",
    "nyc": "^11.1.0",
    "shx": "^0.2.2"
  },
  "keywords": [
    "static-site",
    "jekyll",
    "hexo",
    "blog",
    "wordpress",
    "collections",
    "taxonomy"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ubenzer/fil.git"
  },
  "bugs": {
    "url": "https://github.com/ubenzer/fil/issues"
  },
  "nyc": {
    "include": [
      "app/**"
    ],
    "reporter": [
      "lcov",
      "text",
      "json"
    ]
  }
}
